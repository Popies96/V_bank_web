{% extends 'base.html.twig' %}



{% block body %}
    <h1>Transaction</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>IdTrans</th>
                <td>{{ transaction.idTrans }}</td>
            </tr>
            <tr>
                <th>Expediteur</th>
                <td>{{ transaction.expediteur }}</td>
            </tr>
            <tr>
                <th>Destinataire</th>
                <td>{{ transaction.destinataire }}</td>
            </tr>
            <tr>
                <th>Montant</th>
                <td>{{ transaction.montant }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ transaction.type }}</td>
            </tr>
            <tr>
                <th>Datetemps</th>
                <td>{{ transaction.datetemps }}</td>
            </tr>
            <tr>
                <th>Localisation</th>
                <td>{{ transaction.localisation }}</td>
            </tr>
            <tr>
                <th>Etat</th>
                <td>{{ transaction.etat }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ transaction.description }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_transaction_index') }}">back to list</a>

    <a href="{{ path('app_transaction_edit', {'idTrans': transaction.idTrans}) }}">edit</a>

    {{ include('transaction/_delete_form.html.twig') }}
{% endblock %}
